<Last10Screen>:
    id: screen_last10
    name: "last10"
    last10_table: last10_table
    last10_footer: last10_footer
    on_enter: root.get_last10()
    on_pre_enter: root.attach()
    on_leave: root.detach()
    BoxLayout:
        size_hint: 1, 1
        orientation: 'vertical'
        pos_hint: {'top':1}
        BoxLayout:
            orientation: 'vertical'
            size_hint: 1, 0.9
            GridLayout:
                cols: 6
                size_hint_y:None
                size_hint_x: 1
                height: self.minimum_height
                rows:1
                row_force_default: True
                row_default_height:dp(25)
                Label:
                    text: '[color=000000]#[/color]'
                    markup: True
                    canvas.before:
                        Color:
                            rgba: 1, 1, 1, 1
                        Rectangle:
                            pos: self.pos
                            size: self.size
                Label:
                    text: '[color=000000]ID[/color]'
                    markup: True
                    canvas.before:
                        Color:
                            rgba: 1, 1, 1, 1
                        Rectangle:
                            pos: self.pos
                            size: self.size
                Label:
                    text: '[color=000000]Mark[/color]'
                    markup: True
                    canvas.before:
                        Color:
                            rgba: 1, 1, 1, 1
                        Rectangle:
                            pos: self.pos
                            size: self.size
                Label:
                    text: '[color=000000]Last[/color]'
                    markup: True
                    canvas.before:
                        Color:
                            rgba: 1, 1, 1, 1
                        Rectangle:
                            pos: self.pos
                            size: self.size
                Label:
                    text: '[color=000000]First[/color]'
                    markup: True
                    canvas.before:
                        Color:
                            rgba: 1, 1, 1, 1
                        Rectangle:
                            pos: self.pos
                            size: self.size
                Label:
                    text: '[color=000000]Phone[/color]'
                    markup: True
                    canvas.before:
                        Color:
                            rgba: 1, 1, 1, 1
                        Rectangle:
                            pos: self.pos
                            size: self.size
            Last10RV:
                id: last10_table

        BoxLayout:
            id: last10_footer
            size_hint: 1,0.1
            Button:
                size_hint: 1, 1
                markup: True
                text: 'Cancel'
                on_press: app.root.current = 'search'