#: import FadeTransition kivy.uix.screenmanager.FadeTransition
#: import DropDown kivy.uix.dropdown
#: import Popup kivy.uix.popup.Popup

ScreenManagement:
    transition: FadeTransition()
    MainScreen:
    DeliveryScreen:
    DropoffScreen:
    EditCustomerScreen:
    HistoryScreen:
    Last10Screen:
    NewCustomerScreen:
    PickupScreen:
    ReportsScreen:
    SearchScreen:
    SearchResultsScreen:
    SettingsScreen:

<MainScreen>:
    id:screen_main
    update_label:update_data_label
    username: username
    password: password
    name:"main"
    login_popup: login_popup
    login_button: login_button
    settings_button: settings_button
    reports_button: reports_button
    delivery_button: delivery_button
    dropoff_button: dropoff_button
    update_button: update_button

    ActionBar:
        id: _action_main
        size_hint: 1,0.1
        pos_hint: {'top':1}
        ActionView:
            use_separator: True
            ActionPrevious:
                title: 'Jays Cleaners POS'
                with_previous: False
                on_release: root.manager.current = "main"
            ActionGroup:
                text: 'File'
                mode: 'spinner'
                size_hint_x: None
                width: 250
                ActionButton:
                    text: 'New'
                ActionButton:
                    text: 'Open'
                ActionButton:
                    text: 'Save'
                ActionButton:
                    text: 'Save As'
                ActionButton:
                    text: 'Quit'
    GridLayout:
        cols: 2
        size_hint: 1,0.8
        pos_hint: {'top':0.9}
        GridLayout:
            cols: 2
            size_hint_y: 3
            Button:
                id: settings_button
                on_release: app.root.current = "settings"
                text: "Settings"
                disabled: True
                font_size:'20sp'

            Button:
                id: reports_button
                on_release: app.root.current = "reports"
                text: "Reports"
                disabled: True
                font_size:'20sp'

            Button:
                id: delivery_button
                on_release: app.root.current = "delivery"
                text: "Delivery"
                disabled: True
                font_size:'20sp'

            Button:
                id: login_button
                text: "Login"
                font_size:'20sp'
                on_release: root.login_popup.open()

        Button:
            id: dropoff_button
            disabled: True
            font_size:'20sp'
            on_press: app.root.current = "search"
            on_release: root.search_pre()
            text: "Drop / Pickup"

    Popup:
        id: login_popup
        on_parent: if self.parent == screen_main: screen_main.remove_widget(self)
        title: "Login Screen"
        content: authenticate_grid
        size_hint:(None,None)
        size: '400sp', '200sp'
        BoxLayout:
            id: authenticate_grid
            orientation: 'vertical'
            GridLayout:
                cols: 2
                rows: 2
                row_force_default: True
                row_default_height: '40sp'
                size_hint:1,0.7
                Label:
                    text: "username"
                    size_hint_y: None
                    font_size:'20sp'
                TextInput:
                    id: username
                    font_size:'20sp'
                    hint_text: "Enter username"
                    on_text_validate: root.login()
                    multiline: False
                    padding: [5,5,5,5]
                    text: ''
                Label:
                    text: "password"
                    size_hint_y: None
                    font_size:'20sp'
                TextInput:
                    id: password
                    font_size:'20sp'
                    hint_text: "Enter password"
                    on_text_validate: root.login()
                    password:True
                    multiline: False
                    padding: [5,5,5,5]
                    text: ''
            GridLayout:
                cols: 2
                size_hint:1,0.3
                Button:
                    text: "Cancel"
                    font_size:'20sp'
                    on_release: login_popup.dismiss()
                Button:
                    text: "Login"
                    font_size:'20sp'
                    on_release: root.login()
    Button:
        id: update_button
        text:"Update"
        disabled:False
        on_release: root.test_print()
        size_hint: 0.1,0.1
        pos_hint: {'x':0, 'y':0}
        font_size:'20sp'
    Label:
        id: update_data_label
        text:root.update_info()
        size_hint_y: None
        text_size: self.width, None
        height: self.texture_size[1]
        pos_hint: {'x':0.1, 'y':0}
        font_size:'20sp'

<DeliveryScreen>:
    name:"delivery"
    ActionBar:
        id: _action_delivery
        size_hint: 1,0.1
        pos_hint: {'top':1}
        ActionView:
            use_separator: True
            ActionPrevious:
                title: 'Previous Slide'
                with_previous: False
                on_release: root.manager.current = "main"
                pos_hint: {'left':0}
            ActionGroup:
                text: 'File'
                mode: 'spinner'
                size_hint_x: None
                width: 90
                ActionButton:
                    text: 'New'
                ActionButton:
                    text: 'Open'
                ActionButton:
                    text: 'Save'
                ActionButton:
                    text: 'Save As'
                ActionButton:
                    text: 'Quit'

    Button:
        on_release: app.root.current = "main"
        text: "Back Home"
        font_size: 20
        size_hint: 0.1,0.1
        pos_hint: {'x':0,'y':0}

