    on_enter: root.reset()
    BoxLayout:
        orientation: 'horizontal'
        size_hint: 1,0.1
        pos_hint: {'top':1}
        TextInput:
            id: search
            font_size:'20sp'
            hint_text: "Search"
            multiline: False
