<RackTableButton>:
    id: rack_table_button
    markup: True
    font_size:sp(15)
    disabled: False
    text_size: self.size
    valign: 'middle'
    halign: 'center'
    background_color: ''
    spacing: 0

<RackTableRV@RecycleView>:
    viewclass: 'RackTableButton'
    SelectableRecycleGridLayout:
        bar_width: dp(5)
        default_size: None, dp(50)
        default_size_hint: 1, None
        size_hint_y: None
        height: self.minimum_height
        spacing: 0
        cols:4